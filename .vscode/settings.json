{
    "rust-analyzer.runnableEnv": [
        {
            // Set output levels for `tracing` logging.
            "env": {
                "RUST_BACKTRACE": "1",
                "RUST_LOG": "info,riven=debug"
            }
        }
    ],
    "rust-analyzer.cargo.features": [
        "nightly",
        "tracing",
        "deny-unknown"
    ]
}
